  <portfolios>
    <folder id="35296" name="Global">
      <portfolio id="23484" name="Lombard by rating">
        <include what="chain" id="40166" name="Lombard BBB+" color="Orchid" condition="$LastRating.Rating = [BBB+] AND $Currency = &quot;RUB&quot;" />
        <include what="chain" id="40166" name="Lombard BBB" color="MediumOrchid" condition="$LastRating.Rating = [BBB] AND $Currency = &quot;RUB&quot;" />
        <include what="chain" id="40166" name="Lombard BBB-" color="DeepPink" condition="$LastRating.Rating = [BBB-] AND $Currency = &quot;RUB&quot;" />
      </portfolio>
      <portfolio id="71714" name="Micex New Ratings">
        <include what="chain" id="93560" name="BBB" color="DarkSeaGreen" condition="$LastRating.Rating &gt;= [BBB-] and $LastRating.Rating &lt;= [BBB+] and $LastRating.RatingDate &gt;= #03/08/2013# " />
