<?xml version="1.0" encoding="utf-8"?>
<bonds>
  <!-- Field sets -->
  <field-sets>
    <field-set id="Custom bonds" name="Custom bonds">
      <realtime>
        <field type="CUSTOM">CUSTOM</field>
      </realtime>
    </field-set>
    <field-set id="MICEX" name="Micex bonds description">
      <historical>
        <field type="DATE">DATE</field>
        <field type="LAST">CLOSE</field>
      </historical>
      <realtime>
        <field type="BID">BID</field>
        <field type="ASK">ASK</field>
        <field type="LAST">LAST</field>
        <field type="VWAP">VWAP</field>
        <field type="HIST">CF_CLOSE</field>
        <field type="HIST_DATE">CF_DATE</field>
        <field type="VOLUME">CF_VOLUME</field>
      </realtime>
    </field-set>
    <field-set id="Eurobonds" name="Eurobonds description">
      <realtime>
        <field type="BID">275</field>
        <field type="ASK">393</field>
      </realtime>
      <historical>
        <field type="DATE">DATE</field>
        <field type="LAST">CLOSE</field>
      </historical>
    </field-set>
  </field-sets>
  <!-- Chains -->
  <chains>
    <chain id="1" ric="0#RUTSY=MM" field-set-id="MICEX" name="OFZ" color="Red" />
    <chain id="2" ric="0#RUMOSB=MM" field-set-id="MICEX" name="Moscow" color="Green" curve="True" />
    <chain id="3" ric="0#RUSOVB=MM" field-set-id="MICEX" name="Regional" color="Blue" />
    <chain id="4" ric="0#RUCORP=MM" field-set-id="MICEX" name="Corporate bonds" color="Cyan" enabled="False" />
    <chain id="5" ric="0#RFGOVBONDS=" field-set-id="MICEX" name="Russian eurobonds" color="IndianRed" curve="True" enabled="True" />
  </chains>
  <!-- Lists -->
  <lists>
    <list id="1" name="test" color="Violet" curve="False" field-set-id="MICEX">
      <ric>/RU0A0GKKB7=MM</ric>
      <ric>/RU0A0JR3B1=MM</ric>
      <ric>/RU0A0DF2U3=MM</ric>
      <ric>/RU0A0JTDR3=MM</ric>
      <ric>/RU0A0JS0N1=MM</ric>
      <ric>/RU0A0JTGE4=MM</ric>
      <ric>/RU0A0JT1G2=MM</ric>
      <ric>/RU0A0JPTP1=MM</ric>
      <ric>/RU0A0JRWS5=MM</ric>
      <ric>/RU0A0JP6R2=MM</ric>
    </list>
  </lists>
  <!-- Portfolios -->
  <portfolios>
    <folder id="0" name="Main">
      <folder id="1" name="Specific">
        <portfolio id="82762" name="Regionals by rating">
          <include what="chain" id="3" name="Muni B" color="DarkOrchid" condition="$LastRating.Rating &gt;= [B-] and $LastRating.Rating &lt;= [B+] " />
          <include what="chain" id="3" name="Muni BB" color="Blue" condition="$LastRating.Rating &gt;= [BB-] and $LastRating.Rating &lt;= [BB+] " />
          <include what="chain" id="3" name="Muni BBB" color="CornflowerBlue" condition="$LastRating.Rating &gt;= [BBB-] and $LastRating.Rating &lt;= [BBB+] " />
        </portfolio>
      </folder>
      <folder id="2" name="Stuff">
        <portfolio id="3" name="Mos">
          <include what="chain" id="2" name="Moskau" color="Blue" />
          <include what="custom-bond" id="1" name="Bond 1" color="MidnightBlue" />
          <include what="custom-bond" id="39240" name="Bond 2" color="DarkSlateGray" />
        </portfolio>
        <portfolio id="4" name="Main">
          <include what="chain" id="1" name="All OFZs" />
          <exclude what="list" id="1" />
          <include what="custom-bond" id="1" />
          <!-- todo -->
          <exclude what="ric" name="XXXXX" />
        </portfolio>
      </folder>
      <portfolio id="11" name="Regional w/o Moscow">
        <include what="chain" id="3" />
        <exclude what="chain" id="2" />
        <include what="custom-bond" id="1" />
      </portfolio>
      <portfolio id="76699" name="EUR">
        <include what="chain" id="5" name="Russia Eur" color="ForestGreen" />
      </portfolio>
    </folder>
  </portfolios>
