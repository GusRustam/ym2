  <!-- Portfolios -->
  <portfolios>
    <folder id="0" name="Main">
      <folder id="1" name="By rating">
        <portfolio id="82762" name="Regionals by rating">
          <include what="chain" id="3" name="Muni B" color="DarkOrchid" condition="$LastRating.Rating &gt;= [B-] and $LastRating.Rating &lt;= [B+] " />
          <include what="chain" id="3" name="Muni BB" color="Blue" condition="$LastRating.Rating &gt;= [BB-] and $LastRating.Rating &lt;= [BB+] " />
